		"semantic-release": "^15.12.0",
		"source-list-map": "2.0.1",
		"superagent": "^4.1.0",
		"supertest": "^3.3.0"
	},
	"dependencies": {
		"@sentry/node": "^4.4.1",
		"app-root-path": "^2.1.0",
		"babel-plugin-webpack-alias": "^2.1.2",
		"bcrypt-nodejs": "^0.0.3",
		"body-parser": "^1.18.3",
		"chalk": "^2.4.1",
		"compression": "^1.7.3",
		"cors": "^2.8.5",
		"cpx": "^1.5.0",
		"dotenv": "^6.1.0",
		"ejs": "^2.6.1",
		"email-templates": "2",
		"express": "^4.16.4",
		"express-acl": "^2.0.4",
		"express-brute": "^1.0.1",
		"express-brute-mongoose": "^1.0.2",
		"express-brute-redis": "^0.0.1",
		"express-validation": "^1.0.2",
		"express-winston": "^3.0.1",
		"helmet": "^3.15.0",
		"http-status": "^1.3.1",
		"joi": "^14.0.6",
		"jsonwebtoken": "^8.4.0",
		"method-override": "^3.0.0",
		"mongoose": "^5.3.12",
		"mongoose-beautiful-unique-validation": "^7.1.1",
