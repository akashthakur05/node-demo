		"@babel/preset-env": "^7.1.6",
		"@babel/register": "^7.0.0",
		"apidoc": "^0.17.7",
		"babel-cli": "^6.26.0",
		"babel-eslint": "^10.0.1",
		"babel-preset-env": "^1.7.0",
		"backpack-core": "^0.8.2",
		"base64url": "^3.0.1",
		"chai": "^4.2.0",
		"eslint": "^5.9.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-prettier": "^3.0.0",
		"faker": "^4.1.0",
		"husky": "^1.1.4",
		"jest": "^23.6.0",
		"lint-staged": "^8.0.5",
		"morgan": "^1.9.1",
		"nodemon": "^1.18.6",
		"nps": "^5.9.3",
		"nps-utils": "^1.7.0",
		"nyc": "^13.1.0",
