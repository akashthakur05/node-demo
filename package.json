{
	"name": "nodejs-api-boilerplate",
	"description": "NodeJS API Boilerplate",
	"version": "1.0.3",
	"author": "Truong Vu <truong.vu@codeenginestudio.com>",
	"engines": {
		"node": ">= 8.10.0",
		"npm": ">= 3.10",
		"yarn": ">=0.20.3"
	},
	"main": "./src/index.js",
	"license": "MIT",
	"keywords": [
		"express",
		"es6",
		"es7",
		"rest",
		"api",
		"boilerplate",
		"mongodb",
		"node",
		"javascript",
		"pm2",
		"babel",
		"nps",
		"prettier",
		"lint-staged",
		"jest"
	],
	"config": {
		"testPath": "./__tests__"
	},
	"scripts": {
		"cp": "cpx -c ./nacl.json -c ./.dev ./dist",
		"build": "nps build",
		"debug": "nps debug",
		"dev": "nps dev",
		"start": "nps",
		"stop": "nps stop",
		"test": "nps test",
		"test:cover": "nps test.cover",
		"test:cover:open": "nps test.cover.open",
		"test:path": "npm config set $npm_package_name:testPath",
		"test:watch": "nps test.watch",
		"prettier": "prettier --write '{src,__{tests,mocks}__}/**/*.js'",
		"precommit": "nps lintStaged && nps validate --no-verify",
		"lint": "nps lint",
		"lint:fix": "nps lint.fix",
		"doc": "nps doc",
		"doc:deploy": "nps doc.deploy"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.2.3",
